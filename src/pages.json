{
	"easycom": {
		"^u-(.*)": "uview-ui/components/u-$1/u-$1.vue"
	},
	"pages": [
    //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
    // 启动页
    {
      "path": "pages/generalTable/index",
      "style": {
        "navigationBarTitleText": "通用页",
        "navigationStyle": "custom",
        "enablePullDownRefresh": true
      }
    },
    {
      "path": "pages/building/v1/index",
      "style": {
        "navigationBarTitleText": "厂区管理",
        "navigationStyle": "custom",
        "enablePullDownRefresh": true,
        "navigationBarBackgroundColor": "#3A65FF",
        "app-plus": {
          "bounce": "none"
        }
      }
    },
    {
      "path": "pages/building/v1/buildingType",
      "style": {
        "navigationBarTitleText": "添加场区分类",
        "navigationStyle": "custom",
        "enablePullDownRefresh": false,
        "navigationBarBackgroundColor": "#3A65FF",
        "app-plus": {
          "bounce": "none"
        }
      }
    },
    {
      "path": "pages/building/v1/addBuilding",
      "style": {
        "navigationBarTitleText": "添加场区",
        "navigationStyle": "custom",
        "enablePullDownRefresh": false,
        "navigationBarBackgroundColor": "#3A65FF",
        "app-plus": {
          "bounce": "none"
        }
      }
    },
	{
	  "path": "pages/capacity/vehicle/index",
	  "style": {
	    "navigationBarTitleText": "车辆管理",
	    "navigationStyle": "custom",
	    "enablePullDownRefresh": false,
	    "navigationBarBackgroundColor": "#ffffff",
		"app-plus": {
		  "bounce": "none"
		}
	  }
	},
	{
	  "path": "pages/capacity/driver/index",
	  "style": {
	    "navigationBarTitleText": "司机管理",
	    "navigationStyle": "custom",
	    "enablePullDownRefresh": false,
	    "navigationBarBackgroundColor": "#ffffff",
		"app-plus": {
		  "bounce": "none"
		}
	  }
	}
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "统计",
    "navigationBarBackgroundColor": "#fff",
    "backgroundColor": "#fff"
  }
}
